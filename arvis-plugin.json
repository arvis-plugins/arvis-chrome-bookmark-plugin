{
    "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/plugin-schema.json",
    "creator": "jopemachine",
    "name": "arvis-chrome-bookmark-plugin",
    "enabled": true,
    "description": "Search Google Chrome Bookmark in Arvis",
    "readme": "",
    "version": "0.0.7",
    "webAddress": "https://github.com/arvis-plugins/arvis-chrome-bookmark-plugin",
    "main": "index.js",
    "defaultIcon": "icon.png",
    "variables": {
        "browser": "Chrome",
        "chrome_profile": "Default"
    },
    "actions": [
        {
            "type": "open",
            "modifiers": "normal",
            "target": "{query}"
        }
    ]
}
